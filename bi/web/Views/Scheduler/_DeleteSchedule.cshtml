<div id="delete-popup-container" class="col-lg-12 no-padding">
    <div class="col-md-12 header-menu header-pad-top">
        <div class="col-xs-6 no-padding">
            <span class="su su-delete head-icon"></span>
            <span class="popup-title">@T["Delete Schedule"]</span>
        </div>
        <div class="col-xs-6 no-padding">
            <a href="javascript:void(0);" title="@T["Close"]" class="popup-close"><span class="su su-close pull-right"></span></a>
        </div>
    </div>
    <div class="dialog-body col-xs-12 text-center no-padding">
        <div class="col-xs-12">
            <div class="delete-schedule-content">@Html.Raw(string.Format(T["Are you sure you want to delete this schedule - {0}?"].Value, "<span class='highlight-name'>" + ViewBag.schedulename + "</span>"))</div>
        </div>
    </div>
    <div class="col-xs-12 dialog-footer text-right">
        <div class="col-xs-4"></div>
        <div class="col-xs-8 no-padding text-right validation-area">
            <input type="button" class="critical-action-button app-btn-danger text-right" value="@T["Yes"]" id="delete-item" tabindex="1" autofocus />
            <input type="button" id="Cancel" class="secondary-button app-btn-secondary text-right popup-close pull-right" value="@T["No"]" onclick="deleteScheduleDialogClose()" tabindex="2" />
        </div>
        <div class="col-xs-8 no-padding text-left success-area" style="display:none">
            <input title="" type="button" id="Cancel" class="secondary-button app-btn-secondary left-margin text-right popup-close" value="@T["OK"]" onclick="deleteScheduleDialogClose()" tabindex="3" autofocus />
        </div>
    </div>
</div>
