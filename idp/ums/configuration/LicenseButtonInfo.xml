<ButtonInfos>
  <ButtonInfo>
    <Id>PaymentPending</Id>
    <StatusCode>PaymentPending</StatusCode>
    <StatusMessage>Dashboard site registration is pending</StatusMessage>
    <Button>Continue registration</Button>
    <URL>/setup?id={id}</URL>
  </ButtonInfo>
  <ButtonInfo>
    <Id>DeploymentFailed</Id>
    <StatusCode>DeploymentFailed</StatusCode>
    <StatusMessage>Dashboard site deployment has failed after successful registration</StatusMessage>
    <Button>Submit a support ticket</Button>
    <URL></URL>
  </ButtonInfo>
  <ButtonInfo>
    <Id>Active</Id>
    <StatusCode>Active</StatusCode>
    <StatusMessage>Dashboard site is active</StatusMessage>
    <Button>Manage Subscription</Button>
    <URL>/bi/administration/subscription</URL>
  </ButtonInfo>
  <ButtonInfo>
    <Id>MarkedForSuspension</Id>
    <StatusCode>PaymentPending</StatusCode>
    <StatusMessage>Dashboard site is about to be suspended</StatusMessage>
    <Button>Manage Payments</Button>
    <URL>/bi/administration/payments</URL>
  </ButtonInfo>
  <ButtonInfo>
    <Id>Suspended</Id>
    <StatusCode>PaymentPending</StatusCode>
    <StatusMessage>Dashboard site is suspended</StatusMessage>
    <Button>Manage Payments</Button>
    <URL>/bi/administration/payments</URL>
  </ButtonInfo>
</ButtonInfos>